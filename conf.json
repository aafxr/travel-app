{
  "recurseDepth": 10,
  "source": {
    "includePattern": "\\.(jsx|js|ts|tsx)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "search": true,
    "default": {
      "staticFiles": {
        "include": []
      }
    }
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/better-docs/typescript",
    "node_modules/better-docs/category"
  ],
  "title": "TravelerApp Documentation",
  "opts": {
    "template": "node_modules/better-docs"
  },

  "categoryfile": "jsdoc_category_file.json",

  "better-docs": {
    "name": "TravelerApp",
    "title": "TravelerApp",
    "css": ""
  }
}